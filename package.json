  "description": "A system for processing various types of input (text, tasks, etc.) and routing them to appropriate destinations.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
